---
import Layout from "@layouts/Layout.astro";
import collections from "@lib/collections";
import AncestryPage from "@components/pages/ancestries/AncestryPage.astro";
export async function getStaticPaths() {
    const ancestries = collections.ancestry;
    return ancestries.map((item) => ({
        params: { id: item.id },
        props: { item },
    }));
}

const { item } = Astro.props;
---

<Layout>
    <AncestryPage item={item} />
</Layout>
