---
import Layout from "@layouts/Layout.astro";
import collections from "@lib/collections";
import ArchetypePage from "@components/pages/archetypes/ArchetypePage.astro";
export async function getStaticPaths() {
    const archetypes = collections.archetype;
    return archetypes.map((item) => ({
        params: { id: item.id },
        props: { item },
    }));
}
const { item } = Astro.props;
---

<Layout>
    <ArchetypePage item={item} />
</Layout>
