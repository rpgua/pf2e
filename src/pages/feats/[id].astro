---
import Layout from "@layouts/Layout.astro";
import FeatCard from "@components/cards/FeatCard.astro";
import collections from "@lib/collections";
export async function getStaticPaths() {
    const feats = collections.feat;
    return feats.map((item) => ({
        params: { id: item.id },
        props: { item },
    }));
}

const { item } = Astro.props;
---

<Layout>
    <FeatCard item={item} />
</Layout>
